  background: #fff !important;
  padding: 20px;
}
html 
{
  background: #fff !important;
}
.ui.input {
  width: 100%
}
.ui
{
  margin-top: 10px
}
#dropdown-wrap
{
  text-align: right;
}
.content-wrap
{
  margin-top: 60px !important;
  width: 50%
}
.ui.segment
{
  min-height: 95px;
 /* box-shadow: 0px 1px 2px 0 rgba(34, 36, 38, 0.15); 
